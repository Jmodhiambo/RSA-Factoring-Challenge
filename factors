#!/usr/bin/python3
"""
Module factors
This module factorizes a number.
Functions:
    def main()
"""
import math


def main():
    """Factorizes numbers from a file"""
    try:
        with open("tests/test00", "r") as file:
            for i in file:
                num = int(i.strip())  # Remove any newline characters
                sqrtt = math.isqrt(num)  # Directly compute integer square root

                found_factor = False

                for j in range(2, sqrtt + 1):
                    if num % j == 0:
                        k = num // j  # Use integer division
                        print(f"{num}={j}*{k}")
                        found_factor = True
                        break
                
                # If no factor was found, the number is prime or special case
                if not found_factor:
                    print(f"{num}={num}*1")  # Handle prime numbers or no factor case

    except FileNotFoundError:
        print("The file was not found.")
    except ValueError as ve:
        print(f"Value error: {ve}")
    except Exception as err:
        print(f"Exception: {err}")

if __name__ == "__main__":
    main()
